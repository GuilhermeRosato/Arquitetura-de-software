public class Main
{
public static void main(String[] args) {
//System.out.println("Hello World");
Carro carro1 = new Carro();
Carro carro2 = new Carro();
carro2.modelo = "Corsa";
System.out.println("modelo: "+carro1.modelo);
System.out.println("velocidade m√°xima: "+carro1.velocidadeMax);
System.out.println("chassi: "+carro1.chassi);
System.out.println("modelo: "+carro2.modelo);
carro1.ligar();
carro1.acelerar();
System.out.println("reduzindo velocidade: "+carro1.reduzirVel());
System.out.println("com turbo: "+carro1.turbo(325.6));
}
}




public class Carro{
    public String modelo = "monza";
    public int chassi = 123321;
    public double velocidadeMax = 220.5;
   
    public void ligar(){
        System.out.println("Carro ligado");
    }
    public void acelerar(){
        System.out.println("acelerando vrum vrum");
    }
    public double reduzirVel(){
        return velocidadeMax / 2;
    }
    public double turbo(double v){
        return velocidadeMax + v;
    }
}
